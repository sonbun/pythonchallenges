from turtle import *
ht()
speed(0)
bgcolor("black")
color("white")
pu()
goto(-150,150)
pd()
pensize(5)
for x in range(4):
    fd(300)
    rt(90)
fd(100)
rt(90)
fd(300)
lt(90)
fd(100)
lt(90)
fd(300)
rt(90)
fd(100)
rt(90)
fd(100)
setheading(180)
fd(300)
lt(90)
lt(90)
fd(300)
rt(90)
fd(100)
rt(90)
fd(300)

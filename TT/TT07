from turtle import *
pu()
speed(0)
ht()
pensize(5)
goto(-300,-300)
pd()
fd(600)
pu()
goto(-300,-300)
lt(90)
pd()
fd(500)
pu()
goto(-250,-300)
def drawBar(x):
    pensize(2)
    pd()
    setheading(90)
    fd(x)
    rt(90)
    fd(50)
    rt(90)
    fd(x)
    pu()
drawBar(250)
goto(-150,-300)
drawBar(500)
goto(-50,-300)
drawBar(69)
goto(50,-300)
drawBar(100)
goto(150,-300)
drawBar(200)
